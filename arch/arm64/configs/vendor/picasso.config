# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Local Version
CONFIG_LOCALVERSION_AUTO=y

# Qualcomm Technologies, Inc. Demux device config
CONFIG_V4L_TEST_DRIVERS=y
CONFIG_VIDEO_VIM2M=m
CONFIG_VIDEO_VICODEC=y
CONFIG_IPA_DEBUG=y

# Qualcomm Atheros CLD WLAN module
CONFIG_QCA_CLD_WLAN=y

# DebugFS
CONFIG_DEBUG_FS=n
CONFIG_MSM_TZ_LOG=n
CONFIG_CMA_DEBUGFS=n
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_IPA_UT=n
CONFIG_IOMMU_TESTS=n
CONFIG_QMP_DEBUGFS_CLIENT=n

# exFAT


# erofs
CONFIG_EROFS_FS=y

# Pseudo filesystems
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Boot options
CONFIG_CMDLINE_EXTEND=y
CONFIG_CMDLINE="ramoops_memreserve=4M"

# Camera
CONFIG_MEDIA_CAMERA_SUPPORT=y
CONFIG_MI7250_CAMERA=y

# GPS

# IR
CONFIG_RC_CORE=y
CONFIG_LIRC=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI=y

# LED

# Power
CONFIG_SMB1398_CHARGER=n
CONFIG_FRAME_WARN=4096

# Touchscreen
CONFIG_TOUCH_COUNT_DUMP=y
CONFIG_LAST_TOUCH_EVENTS=y
CONFIG_SPI_XIAOMI_TP=y
CONFIG_TOUCHSCREEN_NT36xxx_HOSTDL_SPI=y
CONFIG_TOUCHSCREEN_NVT_DEBUG_FS=y
CONFIG_TOUCHSCREEN_FTS=n
CONFIG_TOUCHSCREEN_FTS_FOD=n

# fingerprini
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_GOODIX_TA=y
CONFIG_FINGERPRINT_FPC_TEE=y
CONFIG_FINGERPRINT_FP_VREG_CONTROL=y

# Battery
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_ONEWIRE_GPIO=y
CONFIG_BATT_VERIFY_BY_DS28E16=y

# USB
CONFIG_USB_CONFIGFS_RNDIS=y
CONFIG_G7A_CHG=y

# CPU/Task time and stats accounting
CONFIG_TASK_DELAY_ACCT=y

# CPU Frequency scaling
CONFIG_CPU_FREQ_STAT=y


# Scheduler features
CONFIG_UTS_NS=y

# Tegra firmware driver
CONFIG_CRYPTO_SHA512=y
CONFIG_HWCONF_MANAGER=y

# Classification
CONFIG_NET_ACT_NAT=y
CONFIG_NET_ACT_PEDIT=y
CONFIG_NET_ACT_CSUM=y

# SLA Configuration
CONFIG_SLA_STAIC_COLLECT=y

# Frame buffer hardware drivers
CONFIG_FB_VIRTUAL=y

# Qualcomm RmNet extensions
CONFIG_RMNET_PERF=y
CONFIG_RMNET_SHS=y

# DOS/FAT/NT Filesystems
CONFIG_FAT_DEFAULT_IOCHARSET="utf8"
CONFIG_SDFAT_FS=y
CONFIG_SDFAT_SUPPORT_DIR_SYNC=y
CONFIG_SDFAT_CHECK_RO_ATTR=y

# Library routines
CONFIG_HAS_IOMEM=y

# Misc devices
CONFIG_SIMTRAY_STATUS=y

# RCU Debugging
CONFIG_QCOM_RTB=y
CONFIG_QCOM_RTB_SEPARATE_CPUS=y

# GCOV-based kernel profiling
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y

# Other
CONFIG_IOMMU_DEBUG_TRACKING=y
CONFIG_SECURITY_PERF_EVENTS_RESTRICT=y
CONFIG_MEDIA_RC_SUPPORT=y
CONFIG_BATT_VERIFY=y

# KernelSU
CONFIG_KSU=y

# SBalance
CONFIG_IRQ_SBALANCE=y
CONFIG_IRQ_SBALANCE_POLL_MSEC=30000
CONFIG_SBALANCE_EXCLUDE_CPUS="5,6,7"
